{
	"name": "logicrom",
	"title": "LOGICROM Development Platform",
	"description": "Development Platform for 4G LTE, GSM & NBIoT Chipsets",
	"url": "https://logicrom.com/",
	"homepage": "https://logicrom.com/",
	"license": "MIT",
	"keywords": [
		"dev-platform",
		"GSM",
		"NBIoT",
		"Quectel",
		"MT2621",
		"MT2503",
		"MT2625",
		"GSM IoT",
		"SIMCOM"
	],
	"engines": {
		"platformio": "^5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/waybyte/platform-logicrom.git"
	},
	"version": "0.8.1",
	"vendor": "WAYBYTE Solutions",
	"frameworks": {
		"logicromsdk": {
			"package": "framework-logicromsdk",
			"script": "builder/framework/logicromsdk.py",
			"description": "LOGICROM Software Development Kit for 4G LTE, GSM and NB-IoT Modules",
			"homepage": "https://github.com/waybyte/logicromsdk",
			"title": "LOGICROM SDK"
		},
		"arduino": {
			"package": "framework-logicromarduino",
			"script": "builder/framework/arduino.py",
			"description": "Arduino Wiring-based Framework For LOGICROM Platform",
			"homepage": "https://github.com/waybyte/Arduino_LogicROM",
			"title": "Arduino Framework"
		}
	},
	"packages": {
		"toolchain-gccarmnoneeabi": {
			"type": "toolchain",
			"owner": "platformio",
			"version": "~1.80201.0"
		},
		"framework-logicromsdk": {
			"type": "framework",
			"version": "~0.8.0"
		},
		"framework-logicromarduino": {
			"type": "framework",
			"optional": true,
			"version": "~0.5.0"
		},
		"tool-logicromflasher": {
			"type": "uploader",
			"optional": true,
			"version": "~2.3.0"
		}
	},
	"pythonPackages": {
		"pyserial": ">=3.4"
	}
}
